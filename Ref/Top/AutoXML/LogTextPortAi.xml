<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="../../Autocoders/schema/ISF/interface_schema.rng" type="application/xml" schematypens="http://relaxng.org/ns/structure/1.0"?>

<interface name="LogText" namespace="Fw">
    <include_header>Fw/Time/Time.hpp</include_header>
    <include_header>Fw/Log/TextLogString.hpp</include_header>
    <comment>
    Port Type interface arguments.
    </comment>
    <args>
        <arg name="id" type="FwEventIdType" pass_by="value">
        </arg>
        <arg name="timeTag" type="Fw::Time" pass_by="reference">
        </arg>
        <arg name="severity" type="ENUM" pass_by="value">
           <enum name="TextLogSeverity">
              <item name="TEXT_LOG_FATAL"/>
              <item name="TEXT_LOG_WARNING_HI"/>
              <item name="TEXT_LOG_WARNING_LO"/>
              <item name="TEXT_LOG_COMMAND"/>
              <item name="TEXT_LOG_ACTIVITY_HI"/>
              <item name="TEXT_LOG_ACTIVITY_LO"/>
              <item name="TEXT_LOG_DIAGNOSTIC"/>
           </enum>
        </arg>
        <arg name="text" type="Fw::TextLogString" pass_by="reference">
        </arg>
    </args>
</interface>